{
  "name": "node-api",
  "version": "0.1.0",
  "description": "starting something awesome...",
  "main": "src/server.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/jorgeveranet/node-api.git"
  },
  "scripts": {
    "build": "tsc --pretty",
    "lint": "eslint src test",
    "lint:fix": "eslint --fix src test",
    "dev": "NODE_ENV=development nodemon -e ts --exec 'node --inspect -r ts-node/register src/server.ts' --watch ./src/ --watch .env",
    "prestart": "npm run build",
    "start": "node dist/server.js",
    "test": "NODE_ENV=test jest --config=jest.config.js",
    "e2e": "NODE_ENV=test jest --config=jest.config.js --watch test/e2e/api.test.ts"
  },
  "keywords": [
    "REST",
    "api",
    "express",
    "postgres"
  ],
  "author": "Jorge Vera",
  "license": "MIT",
  "devDependencies": {
    "@types/chai-string": "^1.4.1",
    "@types/chai-subset": "^1.3.1",
    "@types/dotenv": "^4.0.3",
    "@types/express": "^4.16.0",
    "@types/jest": "^23.3.7",
    "@types/node": "^12.7.1",
    "@types/sinon": "^5.0.5",
    "@types/sinon-chai": "^3.2.0",
    "@types/supertest": "^2.0.7",
    "chai": "^4.1.2",
    "chai-string": "^1.4.0",
    "chai-subset": "^1.6.0",
    "eslint": "^4.19.1",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-jest": "^21.25.1",
    "eslint-plugin-prettier": "^2.6.0",
    "jest": "^24.8.0",
    "nodemon": "^1.18.5",
    "prettier": "^1.12.1",
    "sinon": "^7.1.0",
    "sinon-chai": "^3.2.0",
    "supertest": "^3.3.0",
    "ts-jest": "^23.10.4",
    "ts-node": "^7.0.1",
    "typescript": "^3.1.3"
  },
  "dependencies": {
    "axios": "^0.18.1",
    "dotenv": "^6.0.0",
    "event-stream": "^3.3.4",
    "express": "^4.16.3"
  }
}
